{
  "conf_dir": "./",
  "name": "Local KrakenD EE",
  "host": ["http://localhost:7979"],
  "port": 7979,
  "grpc_catalog": "../../images/grpc/defs",
  "static_files_dir": "../../data",
  "opentelemetry": {
    "prometheus": {
       "port": 9091
    },
    "tempogrpc": {
        "port": 5317,
        "host": "localhost"
    }
  },
  "endpoints": {
    "ee_websockets": {
        "def": {
            "backend": [
                {
                    "url_pattern": "/ws/{room}",
                    "disable_host_sanitize": true,
                    "host": ["ws://localhost:4245"]
                }
            ]
        }
    },
    "ee_websockets_ee": {
        "def": {
            "backend": [
                {
                    "url_pattern": "/",
                    "encoding": "no-op",
                    "host": ["http://localhost:4245"]
                }
            ]
        }
    },
      "feat_jwt_signing": {
        "extra_config": {
            "signer": {
                "val": {
                    "jwk_url": "http://localhost:8000/jwk/symmetric.json"
                }
            }
        }
      }
  }
}
