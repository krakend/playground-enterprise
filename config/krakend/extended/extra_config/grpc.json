{
    "catalog": [
        "{{.grpc_catalog}}"
    ],
    "server": {
        "services": [
            {
                "name": "flight_finder.Flights",
                "methods": [
                    {
                        "name": "FindFlight",
                        "input_headers": [
                            "x-my-custom-header"
                        ],
                        "backend": [
                            {
                              "host": [
                                "grpc://localhost:4242"
                              ],
                              "url_pattern": "/flight_finder.Flights/FindFlight",
                              "extra_config": {
                                "backend/grpc": {
                                  "use_request_body": true
                                }
                              }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}
